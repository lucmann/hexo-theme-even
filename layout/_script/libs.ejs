<% if (theme.fancybox) { %>
<% var fancybox_js = 'fancybox/jquery.fancybox.pack.js' %>
<% } %>

<%
var js_libs = {
jquery: 'jquery/jquery.min.js',
slideout: 'slideout/slideout.js',
mermaid: 'mermaid/mermaid.min.js',
fancybox: fancybox_js,
}
%>

<% for (var lib in js_libs) { %>
<% if (lib) { %>
<script type="text/javascript" src="<%- url_for('lib') %>/<%- js_libs[lib] %>"></script>
<% } %>
<% } %>

<% if (theme.mermaid.enable) { %>
<script type="text/javascript" src="https://unpkg.com/mermaid@11.2.1/dist/mermaid.min.js"></script>
<script>
    if (window.mermaid) {
        mermaid.initialize(<%- JSON.stringify(theme.mermaid.options) %>);
    }
</script>
<% } %>
